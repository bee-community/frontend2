{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "quoteProps": "as-needed",
  "singleQuote": false,
  "trailingComma": "none",
  "bracketSpacing": true,
  "arrowParents": "always",
  "endOfLine": "auto",
  "htmlWhitespaceSensitivity": "css",
  "embeddedLanguageFormatting": "off",
  "jsxBracketSameLine": true,
  "jsxSingleQuote": false
}
