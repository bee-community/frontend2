{
  "presets": ["@emotion/babel-preset-css-prop"],
  "plugins": [
    [
      "@emotion",
      {
        "autoLabel": "dev-only",
        "labelFormat": "jh-[dirname]-filename-[local]"
      }
    ],
    ["import", { "libraryName": "antd", "style": "css" }],
    "@babel/plugin-proposal-class-properties"
  ]
}
